<Window x:Class="DropdashTimer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DropdashTimer"
        mc:Ignorable="d"
        Title="Dropdash Timer" MouseDoubleClick="MainWindow_OnMouseDoubleClick" Height="350" Width="525">
    <Grid Background="Magenta">
        <TextBlock Name="TimeBlock" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="100px
        ">0:00:00.00</TextBlock>
    </Grid>
    
</Window> 